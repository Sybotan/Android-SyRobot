<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@drawable/robot_little_star_joint_setting"
              android:orientation="horizontal">
    <LinearLayout android:layout_width="wrap_content"
                  android:layout_height="match_parent"
                  android:layout_margin="5dp"
                  android:layout_weight="1"
                  android:orientation="vertical">
        <RadioButton android:id="@+id/uiJointButton10"
                     android:text="10"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="12"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton11"
                     android:text="11"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="13"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton12"
                     android:text="12"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="14"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton13"
                     android:text="13"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="15"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton14"
                     android:text="14"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="16"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton15"
                     android:text="15"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="17"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton16"
                     android:text="16"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="18"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton17"
                     android:text="17"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="19"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton18"
                     android:text="18"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="20"
                     android:layout_weight="1"/>
    </LinearLayout>
    <LinearLayout android:layout_width="wrap_content"
                  android:layout_height="match_parent"
                  android:orientation="horizontal">
        <Button android:id="@+id/uiHomeSettingButton"
                android:text="@string/button_home_setting"
                android:layout_width="wrap_content"
                android:layout_height="100dp"/>
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="match_parent"
                      android:orientation="vertical">
            <Button android:id="@+id/uiMaxButton"
                    android:text="@string/button_max"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <TextView android:id="@+id/uiJointValue"
                      android:text="0"
                      android:textAlignment="center"
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"/>
            <com.sybotan.android.views.VerticalSeekBar
                    android:id="@+id/uiSeekBar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:progress="800"
                    android:max="1600"/>
            <Button android:id="@+id/uiStepButton"
                    android:text="@string/button_step"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <Button android:id="@+id/uiMinButton"
                    android:text="@string/button_min"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout android:layout_width="wrap_content"
                      android:layout_height="match_parent"
                      android:orientation="vertical">
            <Button android:id="@+id/uiUpButton"
                    android:text="加大"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"/>
            <Button android:id="@+id/uiDownutton"
                    android:text="减小"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:layout_width="wrap_content"
                  android:layout_height="match_parent"
                  android:layout_margin="5dp"
                  android:layout_weight="1"
                  android:orientation="vertical">
        <RadioButton android:id="@+id/uiJointButton1"
                     android:text="1"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="0"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton2"
                     android:text="2"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="1"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton3"
                     android:text="3"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="2"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton4"
                     android:text="4"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="3"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton5"
                     android:text="5"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="4"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton6"
                     android:text="6"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="5"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton7"
                     android:text="7"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="6"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton8"
                     android:text="8"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="7"
                     android:layout_weight="1"/>
        <RadioButton android:id="@+id/uiJointButton9"
                     android:text="9"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:contentDescription="8"
                     android:layout_weight="1"/>
    </LinearLayout>
</LinearLayout>
